<main>
    <a href="/teacher/viewPanel" class="btn btn-success back">Back </a>
    <div class="form-container">
    <form id="TeacherUpdate" action="/teacher/viewPanel">
    
        <div class="login">
            <h3>Edit Student</h3>
        </div>

        <div id="error"></div>

        <div class="fields">
            <div class="form-control" >
                <label for="roll_number">Roll Number</label>
                <input type="text" id="roll_number" name="roll_number"  value= <%= roll_number %> Readonly>
            </div>
        
            <div class="form-control">
                <label for="name">name</label>
                <input type="text" id="name" value= <%= name %> name="name" >
            </div>
        
            <div class="form-control">
                <label for="date_of_birth">Date of Birth</label>
                <input type="date" id="date_of_birth" name="date_of_birth" value= <%= date_of_birth %> placeholder="yyyy-mm-dd">
            </div>
        
            <div class="form-control">
                <label for="score">Score</label>
                <input type="text" id="score" value= <%= score %> name="score" >
            </div>
        </div>
    
     <div class="login">
        <button class="btn btn-success" type="submit">Edit Student</button>
    </div>
        <!-- <div class="login">
            <button class="btn btn-success" (click)="handleEditStudent()">Edit Student</button>
           
        </div> -->
        </form>
        </div>
    
    </main>

    <script src="/js/editstudent.js"></script>
    
    
    
    
    
    
    